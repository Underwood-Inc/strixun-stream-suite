{
  "name": "strixun-twitch-api",
  "version": "1.0.0",
  "description": "Cloudflare Worker for Twitch API proxy with cloud storage, authentication, and CDN endpoints",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "tail:prod": "wrangler tail --env production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@strixun/api-framework": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@vitest/coverage-v8": "^4.0.16",
    "vitest": "^4.0.16",
    "wrangler": "^4.54.0"
  }
}

