{
  "name": "@strixun/kv-entities",
  "version": "1.0.0",
  "description": "Unified KV entity storage pattern for all Cloudflare Workers services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./entities": {
      "types": "./dist/entities.d.ts",
      "import": "./dist/entities.js"
    },
    "./indexes": {
      "types": "./dist/indexes.d.ts",
      "import": "./dist/indexes.js"
    },
    "./access": {
      "types": "./dist/access.d.ts",
      "import": "./dist/access.js"
    },
    "./migration": {
      "types": "./dist/migration.d.ts",
      "import": "./dist/migration.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
