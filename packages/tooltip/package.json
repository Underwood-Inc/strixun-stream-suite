{
  "name": "@strixun/tooltip",
  "version": "1.0.0",
  "description": "Reusable tooltip component for Svelte applications",
  "private": true,
  "type": "module",
  "main": "./Tooltip.svelte",
  "types": "./Tooltip.svelte",
  "exports": {
    ".": {
      "types": "./Tooltip.svelte",
      "default": "./Tooltip.svelte"
    },
    "./Tooltip.svelte": "./Tooltip.svelte"
  },
  "files": [
    "*.svelte",
    "*.ts"
  ],
  "peerDependencies": {
    "svelte": "^5.46.0"
  },
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --passWithNoTests"
  },
  "devDependencies": {
    "vitest": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16"
  }
}

