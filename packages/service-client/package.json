{
  "name": "@strixun/service-client",
  "version": "1.0.0",
  "description": "Reusable service-to-service client library for authenticated API calls",
  "private": true,
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./integrity": {
      "types": "./integrity.ts",
      "default": "./integrity.ts"
    },
    "./integrity-response": {
      "types": "./integrity-response.ts",
      "default": "./integrity-response.ts"
    }
  },
  "files": [
    "*.ts",
    "*.md"
  ]
}

