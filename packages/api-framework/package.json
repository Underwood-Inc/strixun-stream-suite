{
  "name": "@strixun/api-framework",
  "version": "1.0.0",
  "description": "Shared API framework for Strixun Stream Suite - Cloudflare Workers and browser support",
  "private": true,
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./enhanced": {
      "types": "./enhanced.d.ts",
      "default": "./enhanced.ts"
    },
    "./client": {
      "types": "./client.d.ts",
      "default": "./client.ts"
    },
    "./enhanced-router": {
      "types": "./enhanced-router.ts",
      "default": "./enhanced-router.ts"
    },
    "./test-utils": {
      "types": "./test-utils.ts",
      "default": "./test-utils.ts"
    }
  },
  "files": [
    "*.js",
    "*.d.ts",
    "*.ts"
  ],
  "dependencies": {
    "@strixun/service-client": "workspace:*"
  }
}

